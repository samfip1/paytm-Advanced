{
    "version": 2,
    "builds": [
      {
        "src": "./backend",
        "use": "@vercel/node",
        "config": {
          "installCommand": "npm install",
          "buildCommand": "npm run build && prisma generate"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "backend/index.js"
      }

    ]
  }